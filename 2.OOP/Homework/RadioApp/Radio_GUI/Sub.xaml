        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Radio_GUI"
        mc:Ignorable="d"
        Title="Sub" Height="306" Width="707">
    <Grid>
        <TextBox x:Name="tBoxName" HorizontalAlignment="Left" Height="35" Margin="232,67,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="401"/>
        <TextBox x:Name="tBoxURL" HorizontalAlignment="Left" Height="35" Margin="232,141,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="401"/>
        <Button x:Name="btnSubmit" Content="Submit" HorizontalAlignment="Center" Height="40" Margin="0,216,0,0" VerticalAlignment="Top" Width="217" Click="btnSubmit_Click"/>
        <Label x:Name="label" Content="Station Name:" HorizontalAlignment="Left" Height="35" Margin="73,67,0,0" VerticalAlignment="Top" Width="128"/>
        <Label x:Name="label2" Content="Station URL:" HorizontalAlignment="Left" Height="35" Margin="73,141,0,0" VerticalAlignment="Top" Width="128"/>

    </Grid>
</Window>
